        <xs:element xmlns:q1="http://schemas.datacontract.org/2004/07/HU_Store" minOccurs="0" name="GetProductsResult" nillable="true" type="q1:ArrayOfProduct" />
      </xs:sequence>
    </xs:complexType>
  </xs:element>
  <xs:element name="GetBalance">
